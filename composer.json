{
    "name": "parziphal/parse",
    "description": "An Eloquent-like interface for Parse",
    "keywords": ["laravel", "parse", "sdk", "parse-sdk", "eloquent"],
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "laravel/framework": "^5.2 || ^6.0 || ^7.0 || ^8.0",
        "parse/php-sdk": "^1.2.0"
    },
    "autoload": {
        "psr-4": {
            "Parziphal\\Parse\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Parziphal\\Parse\\ParseServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1"
    }
}
